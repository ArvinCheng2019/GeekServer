	</message>
	
	<message msgId="105104" name="ResHeartBeat" cache="false" explain="服务器发送心跳">
		<field class="long" name="serverTime" explain="发送时间"/>
	</message>
	
	<message msgId="105107" name="ResLevelUp" cache="false" explain="升级">
		<field class="int" name="level" explain="等级"/>	
	</message>
	
	<message msgId="105119" name="ResUnlockScreen" cache="false" explain="服务器解锁客户端屏幕">
		<field class="int" name="errCode" explain="错误码0:无错误"/>	
		<field class="string" name="desc" explain="错误描述"/>	
	</message>
	
	<message msgId="105120" name="ResAcrossDay" cache="false" explain="跨天">
		<field class="int" name="worldLevel" explain="世界等级"/>
		<field class="int" name="openServerDays" explain="开服天数"/>
	</message>
	
</messages>
