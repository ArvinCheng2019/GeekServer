<?xml version="1.0" encoding="UTF-8"?>
<messages namespace="Role" explain="角色消息">



	<!-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>分割线<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<-->

	<!-- client to server-->
	<message msgId="105201" name="ReqClientInitOver" cache="false" explain="客户端初始化完成">
	</message>

	<message msgId="105202" name="ReqHeartBeat" cache="false" explain="客户端回复心跳">
		<field class="long" name="clientTime" explain="发送时间"/>
	</message>

	<message msgId="105203" name="ReqCheckResCode" cache="false" explain="检查resCode">
		<field class="int" name="code" explain="当前code"/>
	</message>
	
	<!-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>分割线<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<-->

	<message msgId="105103" name="ResPrompt" cache="false" explain="提示信息">
		<field class="int" name="type" explain="提示信息类型（1Tip提示，2跑马灯，3插队跑马灯，4弹窗，5弹窗回到登陆，6弹窗退出游戏）"/>
		<field class="int" name="lanId" explain="提示信息(语言包id 0时直接显示content)"/>
		<field class="string" name="content" explain="填充字段"/>
	</message>
	
	<message msgId="105102" name="ResCheckResCode" cache="false" explain="检查resCode">
		<field class="bool" name="isMatch" explain="code是否匹配"/>
