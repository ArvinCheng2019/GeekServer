<?xml version="1.0" encoding="UTF-8"?>
<messages namespace="Login" explain="登陆消息">
	
	<!-- bean定义 -->
	<struct name="RoleInfo" cache="false" explain="实体信息">
		<field class="long" name="roleId" explain="实体Id" />
		<field class="string" name="roleName" explain="角色名字"/>
		<field class="int" name="level" explain="角色等级"/>
		<field class="int" name="gender" explain="设置性别0隐藏 1男 2女 " />
	</struct>


	<!-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>分割线<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<-->

	<!-- client to server -->
	<message msgId="103201" name="ReqLogin" cache="false" explain="登陆">
		<field class="string" name="userName" explain="登陆用户名"/>
		<field class="int" name="serverId" explain="游戏服务器Id"/>
		<field class="long" name="loginTime" explain="登陆时间"/>
		<field class="string" name="channelId" explain="渠道id"/>
		<field class="int" name="sdkType" explain="0无SDK"/>
		<optional class="string" name="osVersion" explain="手机系统版本"/>
		<optional class="string" name="mobileRand" explain="手机品牌(在IOS中为：ipad iPhone ipod)"/>
		<optional class="string" name="mobileModel" explain="手机型号"/>
		<optional class="string" name="mobileIOS" explain="手机系统 android ios"/>
		<field class="bool" name="isRelogin" explain="是否为后台重连"/>
		<field class="long" name="handToken" explain="登陆token,启动游戏生成一次"/>
	</message>

	<!-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>分割线<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<-->


	<!-- server to client -->
	<message msgId="103101" name="ResLogin" cache="false" explain="登陆">
		<field class="int" name="result" explain="登陆结果（1 = 成功，other = 失败）"/>
		<field class="int" name="reason" explain="登陆失败的原因"/>
		<field class="string" name="username" explain="登陆用户名"/>
		<field class="RoleInfo" name="role" explain="角色信息"/>
		<field class="bool" name="isNewCreat" explain="是否为新角色"/>
	</message>


	<!--短线重连-->
	<message msgId="103205" name="ReqReLogin" cache="false" explain="请求重连">
		<field class="string" name="token" explain="token"/>
	</message>
	<message msgId="103105" name="ResReLogin" cache="false" explain="重连连结果">
		<field class="bool" name="success" explain="登陆结果"/>
	</message>
	
</messages>
