using Geek.Server;
{{~ for use in usingspaces ~}}
using {{use}};
{{~ end ~}}

namespace {{space}}
{
    public class {{name}} : {{super}}
	{
		{{~ for mth in methods ~}}
		
		{{mth.declare}}
		{
			{{~ if !mth.isawait ~}}{{~#************************************************~}}

			long callChainId = base.Actor.WorkerActor.NewChainId();
			_ = base.Actor.WorkerActor.Enqueue(()=>{return base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}});}, callChainId, {{mth.executetime}});
			return Task.CompletedTask;

			{{~ else ~}}{{~#************************************************~}}
			(bool needEnqueue, long chainId)= base.Actor.WorkerActor.IsNeedEnqueue();
			if (!needEnqueue)
			{
				return base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}});
			}
			return base.Actor.WorkerActor.Enqueue(()=>{return base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}});}, chainId, {{mth.executetime}});
			{{~ end ~}}{{~#************************************************~}}
		}

		{{~ end ~}}
	}
}